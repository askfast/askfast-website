doctype
html.no-js
  head
    meta(charset='UTF-8')
    title ASK Community Systems
    meta(name='author', content='ASK Community Systems')
    meta(name='keywords', content='flexwerkers, postnl, knrm, vrijwilligers, zelfsturend, clienten, uitzendkracht, intercedent')
    meta(name='description', content='Lees de ervaringen van gebruikers van het ASK platform. Het ASK platform biedt uw organisatie vele mogelijkheden en voordelen.')
    meta(name='Resource-type', content='Document')
    meta(content='width=device-width, initial-scale=1', name='viewport')
    meta(content='ASK Community Systems', property='og:title')
    meta(content='image', property='og:type')
    meta(content='https://ask-cs.nl/', property='og:url')
    meta(content='', property='og:image')
    link(href='/images/favicon.ico', rel='Shortcut Icon')



    // --------CSS------
    // build:css styles/vendor.css
    // bower:css
    link(rel='stylesheet', href='vendors/animate.css/animate.css')
    // endbower
    // endbuild
    // build:css(.tmp) styles/main.css
    link(rel='stylesheet', href='styles/main.css')
    // endbuild



  body(ng-app='AskWeb')



    //--------HEADER------
    #menu.mainMenu
      #dropdownMenu.mobileView.hide
        ul
          li
            a(href='platform.html') {{ui.menu.platform}}
          li
            a#menuProducts(href='index.html#products') {{ui.menu.products}}
          li
            a(href='about.html') {{ui.menu.company}}
          li.active
            a.active(href='cases.html') {{ui.menu.cases}}
      .container
        .row
          .col-xs-5.desktopView
            ul
              li
                a(href='platform.html') {{ui.menu.platform}}
              li
                a#menuProducts(href='index.html#products') {{ui.menu.products}}
          #mainLogo
            .logoContainer
              a(href='index.html')
          .col-xs-5.col-xs-offset-2.desktopView
            ul
              li
                a(href='about.html') {{ui.menu.company}}
              li
                a.active(href='cases.html') {{ui.menu.cases}}
          #dropdownToggle.mobileView



    //--------COOKIE MESSAGE------
    #cookieMessage
      #closeCookie
      p
        | {{ui.general.cookieMessage}}
        a(href='docs/Privacy-Statement-ASK-Community-Systems.pdf', target='_blank')  {{ui.general.cookieMessageLink}}
        | .


    //--------Cases------
    #content
      #skrollr-body
        //--------PostNL------
        section#slide-1.casePostnl.homeSlide(anim-detached='true')
          .bcg(data-center='background-position: 50% 100%;', data-top-bottom='background-position: 50% -50px;', data-anchor-target='#slide-1')
            .container.smallDeviceContainer(data-center='opacity: 1', data-0-top='opacity: 0.5', data-anchor-target='#slide-1 h2')
              .row
                .textFieldContainer
                  .textField.text-center.col-md-6.col-md-offset-6.col-sm-8.col-sm-offset-4(class="col-xs-{{ui.general.columnSize}} col-xs-offset-{{ui.general.columnSizeOffset}}")
                    .col-md-12
                      h2
                        span {{ui.cases.postnl.title}}
                      h2 {{ui.cases.postnl.factsTitle}}
                    .row
                      .fact.col-md-4.col-xs-12.col-sm-4
                        h1 6
                        p {{ui.cases.postnl.fact01}}
                      .fact.col-md-4.col-xs-12.col-sm-4
                        h1 2570
                        p {{ui.cases.postnl.fact02}}
                      .fact.col-md-4.col-xs-12.col-sm-4
                        h1 170.000
                        p.wordBreak {{ui.cases.postnl.fact03}}
        section#slide-2.casePostnlText.caseTextPart
          .bcg(data-anchor-target='#slide-2')
            .container.smallDeviceContainer
              .row
                .col-md-8.col-sm-7.col-xs-12
                  .row
                    .col-md-12
                      h2 {{ui.cases.postnl.title}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.problemTitle}}
                      ol
                        li {{ui.cases.postnl.problem.point01}}
                        li {{ui.cases.postnl.problem.point02}}
                        li {{ui.cases.postnl.problem.point03}}
                      h4 {{ui.cases.goalsTitle}}
                      ol
                        li {{ui.cases.postnl.goals.point01}}
                        li {{ui.cases.postnl.goals.point02}}
                        li {{ui.cases.postnl.goals.point03}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.solutionTitle}}
                      p {{ui.cases.postnl.solution.description01}}
                      p {{ui.cases.postnl.solution.description02}}
                  .row
                    .col-md-6(data-106-bottom='opacity: 0', data-center='opacity: 1')
                      img(src='images/cases/postnl.png', style='height: auto; width: 100%; margin: 20px 0;', alt='PostNL')
                .textFieldContainer
                  .textField.text-left.col-md-4.col-sm-5.col-xs-12
                    h4 {{ui.cases.qualResultsTitle}}
                    ol
                      li {{ui.cases.postnl.qualResults.point01}}
                      li {{ui.cases.postnl.qualResults.point02}}
                      li {{ui.cases.postnl.qualResults.point03}}
                      li {{ui.cases.postnl.qualResults.point04}}
                    h4.factVerTitle {{ui.cases.quanResultsTitle}}
                    .factVer
                      h1 - 250 FTE's
                      p {{ui.cases.postnl.quanResults.fact01}}
                    .factVer
                      h1 - 45%
                      p {{ui.cases.postnl.quanResults.fact02}}
                    .factVer
                      h1 - â‚¬10.000.000
                      p {{ui.cases.postnl.quanResults.fact03}}
        //--------KNRM------
        section#slide-3.caseKnrm.homeSlide(anim-detached='true')
          .bcg(data-center='background-position: 50% 100%;', data-top-bottom='background-position: 50% -200px;', data-anchor-target='#slide-3')
            .container.smallDeviceContainer
              .row
                .textFieldContainer
                  .textField.text-center.col-md-4.col-sm-6.col-xs-8(data-50-bottom='opacity: 0.5', data-center='opacity: 1', data-0-top='opacity: 0.5')
                    .col-md-12
                      h2
                        span {{ui.cases.knrm.title}}
                      h2 {{ui.cases.knrm.factsTitle}}
                    .row
                      .fact.col-md-6.col-sm-6
                        h1 46
                        p {{ui.cases.knrm.fact01}}
                      .fact.col-md-6.col-sm-6
                        h1 1300
                        p.wordBreak {{ui.cases.knrm.fact02}}
        section#slide-4.caseKnrmText.caseTextPart
          .bcg(data-anchor-target='#slide-4')
            .container.smallDeviceContainer
              .row
                .col-md-8.col-sm-7.col-xs-12
                  .row
                    .col-md-12
                      h2 {{ui.cases.knrm.title}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.problemTitle}}
                      ol
                        li {{ui.cases.knrm.problem.point01}}
                        li {{ui.cases.knrm.problem.point02}}
                        li {{ui.cases.knrm.problem.point03}}
                      h4 {{ui.cases.goalsTitle}}
                      ol
                        li {{ui.cases.knrm.goals.point01}}
                        li {{ui.cases.knrm.goals.point02}}
                        li {{ui.cases.knrm.goals.point03}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.solutionTitle}}
                      p {{ui.cases.knrm.solution.description01}}
                  .row
                    .col-md-6(data-106-bottom='opacity: 0', data-center='opacity: 1')
                      img(src='images/cases/knrm.png', style='height: auto; width: 100%; margin: 20px 0;', alt='KNRM')
                .textFieldContainer
                  .textField.text-left.col-md-4.col-sm-5.col-xs-12
                    h4 {{ui.cases.qualResultsTitle}}
                    ol
                      li {{ui.cases.knrm.qualResults.point01}}
                      li {{ui.cases.knrm.qualResults.point02}}
                      li {{ui.cases.knrm.qualResults.point03}}
        //--------Buurtzorg------
        section#slide-5.caseBz.homeSlide(anim-detached='true')
          .bcg(data-center='background-position: 0% 0px;', data-bottom-top='background-position: 0% 100px;', data-top-bottom='background-position: 0% -100px;', data-anchor-target='#slide-5')
            .container.smallDeviceContainer
              .row
                .textFieldContainer
                  .textField.text-center.col-md-4.col-md-offset-8.col-sm-6.col-sm-offset-6(class="col-xs-{{ui.general.columnSize}} col-xs-offset-{{ui.general.columnSizeOffset}}", data-50-bottom='opacity: 0.5', data-center='opacity: 1', data-0-top='opacity: 0.5')
                    .col-md-12
                      h2
                        span {{ui.cases.bz.title}}
                      h2 {{ui.cases.bz.factsTitle}}
                    .row
                      .fact.col-md-6.col-sm-6
                        h1 12
                        p {{ui.cases.bz.fact01}}
                      .fact.col-md-6.col-sm-6
                        h1 100+
                        p.wordBreak {{ui.cases.bz.fact02}}
        section#slide-6.caseBzText.caseTextPart
          .bcg(data-anchor-target='#slide-6')
            .container.smallDeviceContainer
              .row
                .col-md-8.col-sm-7.col-xs-12
                  .row
                    .col-md-12
                      h2 {{ui.cases.bz.title}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.problemTitle}}
                      ol
                        li {{ui.cases.bz.problem.point01}}
                        li {{ui.cases.bz.problem.point02}}
                      h4 {{ui.cases.goalsTitle}}
                      ol
                        li {{ui.cases.bz.goals.point01}}
                        li {{ui.cases.bz.goals.point02}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.solutionTitle}}
                      p {{ui.cases.bz.solution.description01}}
                      p {{ui.cases.bz.solution.description02}}
                  .row
                    .col-md-6(data-106-bottom='opacity: 0', data-center='opacity: 1')
                      img(src='images/cases/buurtzorg.png', style='height: auto; width: 100%; margin: 20px 0;', alt='Buurtzorg')
                .textFieldContainer
                  .textField.text-left.col-md-4.col-sm-5.col-xs-12
                    h4 {{ui.cases.qualResultsTitle}}
                    ol
                      | {{ui.cases.bz.qualResults.description01}}
                      li {{ui.cases.bz.qualResults.point01}}
                      li {{ui.cases.bz.qualResults.point02}}
                      li {{ui.cases.bz.qualResults.point03}}
                      li {{ui.cases.bz.qualResults.point04}}
        //--------Olympia------
        section#slide-7.caseOlympia.homeSlide
          .bcg(data-center='background-position: 0px 50%;', data-bottom-top='background-position: 0px 50%;', data-top-bottom='background-position: -40px 50%;', data-anchor-target='#slide-7')
            .container.smallDeviceContainer
              .row
                .textFieldContainer
                  .textField.text-center.col-md-4.col-sm-6.col-xs-8(data-50-bottom='opacity: 0.5', data-center='opacity: 1', data-0-top='opacity: 0.5')
                    .col-md-12
                      h2
                        span {{ui.cases.olympia.title}}
                      h2 {{ui.cases.olympia.factsTitle}}
                    .row
                      .fact.col-md-6.col-sm-6
                        h1 20.000
                        p {{ui.cases.olympia.fact01}}
                      .fact.col-md-6.col-sm-6
                        h1 30.000
                        p.wordBreak {{ui.cases.olympia.fact02}}
        section#slide-8.caseOlympiaText.caseTextPart
          .bcg(data-anchor-target='#slide-8')
            .container.smallDeviceContainer
              .row
                .col-md-8.col-sm-7.col-xs-12
                  .row
                    .col-md-12
                      h2 {{ui.cases.olympia.title}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.problemTitle}}
                      ol
                        li {{ui.cases.olympia.problem.point01}}
                        li {{ui.cases.olympia.problem.point02}}
                        li {{ui.cases.olympia.problem.point03}}
                      h4 {{ui.cases.goalsTitle}}
                      ol
                        li {{ui.cases.olympia.goals.point01}}
                        li {{ui.cases.olympia.goals.point02}}
                        li {{ui.cases.olympia.goals.point03}}
                    .col-md-6.col-sm-12.col-xs-12
                      h4 {{ui.cases.solutionTitle}}
                      p {{ui.cases.olympia.solution.description01}}
                      p {{ui.cases.olympia.solution.description02}}
                      p {{ui.cases.olympia.solution.description03}}
                  .row
                    .col-md-6(data-300-bottom='opacity: 0', data-center='opacity: 1')
                      img(src='images/cases/olympia.png', style='height: auto; width: 100%; margin: 20px 0;', alt='Olympia')
                .textFieldContainer
                  .textField.text-left.col-md-4.col-sm-5.col-xs-12
                    h4 {{ui.cases.qualResultsTitle}}
                    ol
                      li {{ui.cases.olympia.qualResults.point01}}
                      li {{ui.cases.olympia.qualResults.point02}}
                      li {{ui.cases.olympia.qualResults.point03}}
                    h4.factVerTitle {{ui.cases.quanResultsTitle}}
                    .factVer
                      h1 + 20%
                      p {{ui.cases.olympia.quanResults.fact01}}
                    .factVer
